<div class="col-md-6 col-md-offset-2">
  <%= form_tag users_list_path, method: 'GET', :remote => true do %>

  <p><%= text_field_tag :search, params[:search]%>
     <%= submit_tag "Search"%></p>
     </div>
<% end %>
<div id="people_grid">
  <p><%= render partial: 'users/users_list', :locals => {users: @users} %></p>
</div>
