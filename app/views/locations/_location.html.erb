<% locationable.locations.each do |location| %>
 <p><%= location.suburb %></p>
 <div id="map"></div>
  <script>
    var map;
    var myLatLng = {lat: <%= location.latitude %>, lng: <%= location.longitude %>}
    function initMap() {
      map = new google.maps.Map(document.getElementById('map'), {
        center: myLatLng,
        zoomControl: true,
        zoom: 15
      });
      var marker = new google.maps.Marker({
      position: myLatLng,
      map: map,
      title: 'Hello World!'
    });
    }
  </script>
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB2WS0F0KWSPoe5cIsGq4a5fUQmnSBXqQ4&callback=initMap"
  async defer></script>


<% end %>
