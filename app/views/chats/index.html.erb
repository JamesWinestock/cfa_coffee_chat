<%= form_tag chats_list_path, method: 'get' do %>
<p><%= text_field_tag :search, params[:search]%> <%= submit_tag "Search", name: nil%></p>
<% end %>

<% @chats.each do |chat| %>
<div class="card">

 <h4 class="card-title"><%= link_to chat.title, chat_path(chat) %> Created by: <%= chat.user.username %></h4>
 <p class="card-text"><%= chat.description %></p>
 <br>
 </div>
<% end %>
